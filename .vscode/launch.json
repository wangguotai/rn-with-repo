{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Expo Start",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "start"],
        "port": 19000, // Expo 默认端口
        "skipFiles": ["<node_internals>/**"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "name": "Attach to Expo",
        "type": "node",
        "request": "attach",
        "port": 19000,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }